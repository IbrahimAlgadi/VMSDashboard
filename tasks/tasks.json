    "javascript_architecture": {
      "design_patterns": {
        "module_pattern": {
          "description": "Encapsulate functionality in modules",
          "example": "const Module = (function() { return { init, destroy }; })();"
        },
        "singleton_pattern": {
          "description": "Single instance for state management",
          "example": "class StateManager { static instance; static getInstance() {} }"
        },
        "observer_pattern": {
          "description": "Event-driven communication",
          "example": "class EventEmitter { on(), off(), emit() }"
        },
        "factory_pattern": {
          "description": "Create component instances",
          "example": "class ComponentFactory { createCard(), createTable() }"
        }
      },
      "core_modules": {
        "App": {
          "file": "app.js",
          "responsibilities": [
            "Initialize application",
            "Load configuration",
            "Setup event listeners",
            "Initialize components",
            "Handle routing"
          ],
          "methods": [
            "init()",
            "loadPage(pageName)",
            "setupEventListeners()",
            "initializeComponents()",
            "destroy()"
          ]
        },
        "StateManager": {
          "file": "state.js",
          "responsibilities": [
            "Manage application state",
            "Persist state to storage",
            "Notify subscribers of changes"
          ],
          "methods": [
            "setState(key, value)",
            "getState(key)",
            "subscribe(key, callback)",
            "unsubscribe(key, callback)",
            "persist()",
            "restore()"
          ]
        },
        "APIClient": {
          "file": "api.js",
          "responsibilities": [
            "Handle API requests",
            "Manage authentication",
            "Handle errors",
            "Cache responses"
          ],
          "methods": [
            "get(endpoint, params)",
            "post(endpoint, data)",
            "put(endpoint, data)",
            "delete(endpoint)",
            "setAuthToken(token)",
            "handleError(error)"
          ]
        },
        "WebSocketManager": {
          "file": "websocket.js",
          "responsibilities": [
            "Manage WebSocket connection",
            "Handle real-time updates",
            "Reconnection logic"
          ],
          "methods": [
            "connect(url)",
            "disconnect()",
            "send(message)",
            "on(event, handler)",
            "reconnect()"
          ]
        }
      },
      "component_classes": {
        "DataTableComponent": {
          "file": "components/datatable.js",
          "extends": "BaseComponent",
          "properties": [
            "element",
            "options",
            "dataTable",
            "data"
          ],
          "methods": [
            "init()",
            "render()",
