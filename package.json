{
  "name": "cctv-dashboard",
  "version": "1.0.0",
  "description": "CCTV Camera Dashboard - Bootstrap VMS Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test",
    "test:ws:nvr": "node test-nvr-websocket.js",
    "test:ws:dashboard": "node test-dashboard-websocket.js",
    "db:create": "node migration.js create",
    "db:check": "node migration.js check",
    "db:migrate": "node migration.js migrate",
    "db:setup": "node migration.js setup",
    "db:drop": "node migration.js drop",
    "db:reset": "node migration.js reset",
    "db:seed": "node seeders/index.js",
    "db:seed:reset": "node seeders/index.js reset",
    "db:seed:list": "node seeders/index.js list",
    "db:seed:users": "node seeders/index.js specific users",
    "db:seed:regions": "node seeders/index.js specific regions",
    "db:seed:branches": "node seeders/index.js specific branches",
    "db:seed:nvrs": "node seeders/index.js specific nvrs",
    "db:seed:cameras": "node seeders/index.js specific cameras",
    "db:seed:alerts": "node seeders/index.js specific alerts"
  },
  "keywords": [
    "cctv",
    "dashboard",
    "bootstrap",
    "express",
    "nunjucks"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nunjucks": "^3.2.4",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4"
  }
}
