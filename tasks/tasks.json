        "icons": {
          "library": "Bootstrap Icons",
          "version": "1.11+",
          "fallback": "Font Awesome 6"
        },
        "themes": {
          "light": "Default Bootstrap theme",
          "dark": "Bootstrap dark mode with custom variables",
          "custom": "CSS variables for brand colors"
        }
      },
      "javascript_libraries": {
        "dom_manipulation": {
          "native": "querySelector, addEventListener",
          "utilities": "Custom helper functions"
        },
        "ajax": {
          "primary": "Fetch API",
          "fallback": "XMLHttpRequest"
        },
        "state_management": {
          "pattern": "Singleton State Manager",
          "storage": "LocalStorage + SessionStorage"
        },
        "routing": {
          "type": "Server-side with hash routing fallback",
          "library": "Custom Router class (optional)"
        }
      },
      "third_party_libraries": {
        "charts": {
          "name": "Apache ECharts",
          "version": "5.4+",
          "loading": "CDN or local",
          "modules": ["core", "charts", "components"]
        },
        "maps": {
          "name": "Leaflet",
          "version": "1.9+",
          "loading": "CDN or local",
          "plugins": ["MarkerCluster", "Heat"]
        },
        "data_tables": {
          "name": "DataTables",
          "version": "1.13+",
          "extensions": ["Responsive", "Buttons", "Select"]
        },
        "date_handling": {
          "name": "Day.js",
          "version": "1.11+",
          "plugins": ["relativeTime", "timezone"]
        },
        "notifications": {
          "name": "Toastr",
          "version": "2.1+",
          "alternative": "Bootstrap Toasts"
        },
        "validation": {
          "name": "Parsley.js",
          "version": "2.9+",
          "alternative": "Native Constraint Validation API"
        },
        "animations": {
          "name": "AOS (Animate On Scroll)",
          "version": "2.3+",
          "alternative": "CSS animations"
        },
        "utilities": {
          "lodash": "4.17+ (debounce, throttle)",
          "axios": "1.6+ (optional for API calls)"
        }
      },
      "build_tools": {
        "optional": true,
        "bundler": "Webpack or Parcel (optional)",
        "task_runner": "Gulp (optional)",
        "package_manager": "npm or yarn",
        "development_server": "Live Server or http-server"
